{
  "name": "marc-lally-cv",
  "version": "1.0.0",
  "description": "A CV and skills profile",
  "license": "MIT",
  "author": "Marc Lally",
  "type": "commonjs",
  "main": "index.html",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir -p dist dist/cv && cp index.html index.css styles.css favicon.ico favicon.svg avatar.jpg dist/ && cp -R cv/. dist/cv/",
    "start": "npm run build && sh -c 'python3 -m http.server ${PORT:-4173} --directory dist'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "portfolio",
    "senior-engineer",
    "javascript",
    "typescript",
    "node",
    "react",
    "serverless",
    "aws"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "globals": "^17.2.0"
  }
}
